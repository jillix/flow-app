{
    "appId": "00000000000000000000000000000002",
    "name": "MonoDev",
    "routes": {
        "/": "dev_layout",
        "deployer": "dev_deployer_layout",
        "applications": "dev_applications_layout"
    },
    "domains": [
        "dev.jillix.net",
        "dev.mono.ch"
    ],
    "scripts": [
    ],
    "publicDir": "public",
    "publicUser": 0,
    "users": [
        {
            "id": 0,
            "username": "",
            "password": "",
            "data": {
                "fullname": "MonoDev Public User"
            },
            "roles": [0]
        }
    ],
    "roles": [
        {
            "id": 0,
            "name": "MonoDev Public Role"
        }
    ],
    "miids": {
        "dev_layout": {
            "module": "github/adioo/layout/v0.0.4",
            "roles": [0],
            
            "config": {
                "html": {
                    "type": "a",
                    "path": "html/layout"
                },
                "css": ["http://blueimp.github.com/cdn/css/bootstrap.min.css"],
                "title": "Mono Dev",
                "modules": {
                    "tabContainer": "tabs"
                }
            }
        },
        "tabs": {
            "module": "github/adioo/bind-tab/v0.0.2",
            "roles": [0],
            "config": {
                "html": {
                    "type": "a",
                    "path": "html/tabs"
                },
                "container": "#container",
                "tabs": "li",
                "options": {
                    "classes": {
                        "selected": "active"
                    },
                    "first": "dev_home_layout",
                    "hash": true
                }
            }
        },
        "dev_home_layout": {
           "module": "github/adioo/layout/v0.0.4",
           "roles": [0],
           "config": {
               "html": {
                   "type": "a",
                   "path": "html/home"
               },
               "css": ["http://blueimp.github.com/cdn/css/bootstrap.min.css", "http://blueimp.github.com/cdn/css/bootstrap-responsive.min.css", "css/home.css"]
           }
       },
       
        "dev_deployer_layout": {
           "module": "github/adioo/layout/v0.0.4",
           "roles": [0],
           "config": {
               "html": {
                   "type": "a",
                   "path": "html/layout_deployer"
               },
               "css": ["http://blueimp.github.com/cdn/css/bootstrap.min.css", "http://blueimp.github.com/cdn/css/bootstrap-responsive.min.css", "css/deployer.css"],
               "modules": {
                   "deployer": "dev_deployer"
               }
           }
       },
       "dev_applications_layout": {
           "module": "github/adioo/layout/v0.0.4",
           "roles": [0],
           "config": {
               "html": {
                   "type": "a",
                   "path": "html/layout_applications"
               },
               "css": ["http://blueimp.github.com/cdn/css/bootstrap.min.css", "http://blueimp.github.com/cdn/css/bootstrap-responsive.min.css"],
               "modules": {
                   "applications": "dev_applications"
               },
               "scripts": [
                   "/jquery.js"
               ],
               "css": ["http://blueimp.github.com/cdn/css/bootstrap.min.css", "http://blueimp.github.com/cdn/css/bootstrap-responsive.min.css"]
           }
       }, 
       "dev_deployer": {
            "module": "github/gabipetrovay/dev_deployer/dev",
            "roles": [0],
            "config": {
                "html": {
                    "type": "a",
                    "path": "html/deployer"
                },
                "css": ["http://blueimp.github.com/Bootstrap-Image-Gallery/css/bootstrap-image-gallery.min.css", "uploader/css/jquery.fileupload-ui.css"]
            },
            "operations": {
                "upload": {
                    "roles": [0],
                    "params": [{ "uploadDir": "uploads" }]
                },
                "deployFromGithub": {
                    "roles": [0],
                    "params": []
                }
            }
        },
        "dev_applications": {
            "module": "github/IonicaBizau/dev_applications/dev",
            "roles": [0],
            "config": {
                "html": {
                    "type": "a",
                    "path": "html/applications"
                },
                "css": ["http://blueimp.github.com/Bootstrap-Image-Gallery/css/bootstrap-image-gallery.min.css", "uploader/css/jquery.fileupload-ui.css", "css/applications.css"]
            },
            "operations": {
                "redeploy": {
                    "roles": [0],
                    "params": []
                },
                "redeployMonoDev": {
                    "roles": [0],
                    "params": []
                },
                "start": {
                    "roles": [0],
                    "params": []
                },
                "stop": {
                    "roles": [0],
                    "params": []
                },
                "update": {
                    "roles": [0],
                    "params": []
                },
                "delete": {
                    "roles": [0],
                    "params": []
                },
                "applications": {
                    "roles": [0],
                    "params": []
                }
            }
        }
    }
}

